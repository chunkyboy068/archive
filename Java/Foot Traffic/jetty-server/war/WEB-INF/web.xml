<?xml version="1.0" encoding="UTF-8"?>	
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"	
	 xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/
javaee/web-app_2_5.xsd"	
	 xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/
javaee/web-app_3_0.xsd"	
	 id="WebApp_ID" version="3.0">	
	 <display-name>Tiny-Jetty-App</display-name>	
	
	 <servlet>	
	 	<servlet-name>MainServlet</servlet-name>	
	 	<servlet-class>server_src.MainServlet</servlet-class>	
	 </servlet>	
	 <servlet>	
	 	<servlet-name>PostData</servlet-name>	
	 	<servlet-class>server_src.PostData</servlet-class>	
	 </servlet>	
	 <servlet>	
	 	<servlet-name>GetData</servlet-name>	
	 	<servlet-class>server_src.GetData</servlet-class>	
	 </servlet>	
	
	 <servlet-mapping>	
	 	<servlet-name>MainServlet</servlet-name>	
	 	<url-pattern>/</url-pattern>	
	 </servlet-mapping>	
	 <servlet-mapping>	
	 	<servlet-name>PostData</servlet-name>	
	 	<url-pattern>/postdata</url-pattern>	
	 </servlet-mapping>	
	 <servlet-mapping>	
	 	<servlet-name>GetData</servlet-name>	
	 	<url-pattern>/getdata</url-pattern>	
	 </servlet-mapping>	
	
</web-app>