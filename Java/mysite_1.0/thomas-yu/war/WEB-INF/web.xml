<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/
javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/
javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>Thomas Yu</display-name>

	<servlet>
		<servlet-name>Index</servlet-name>
		<servlet-class>servlets.Index</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Skills</servlet-name>
		<servlet-class>servlets.Skills</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Education</servlet-name>
		<servlet-class>servlets.Education</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>About</servlet-name>
		<servlet-class>servlets.About</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Contact</servlet-name>
		<servlet-class>servlets.Contact</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Activity</servlet-name>
		<servlet-class>servlets.Activity</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Archive</servlet-name>
		<servlet-class>servlets.Archive</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>FinePrint</servlet-name>
		<servlet-class>servlets.FinePrint</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>default</servlet-name>
		<servlet-class>org.eclipse.jetty.servlet.DefaultServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>Index</servlet-name>
		<url-pattern>/</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Skills</servlet-name>
		<url-pattern>/skills</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Education</servlet-name>
		<url-pattern>/education</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>About</servlet-name>
		<url-pattern>/about</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Contact</servlet-name>
		<url-pattern>/contact</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Activity</servlet-name>
		<url-pattern>/activity</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Archive</servlet-name>
		<url-pattern>/archive</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>FinePrint</servlet-name>
		<url-pattern>/fineprint</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>default</servlet-name>
		<url-pattern>/site_src/*</url-pattern>
	</servlet-mapping>

</web-app>